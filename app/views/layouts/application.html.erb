<!DOCTYPE html>
<html>
<head>
  <title>Rails2</title>
  <%= stylesheet_link_tag :all %>
  <%= javascript_include_tag :defaults %>
  <%= csrf_meta_tag %>
</head>
<body>
<% if !flash.empty? %>
  <% flash.each do |key,value| %>
    <div id="<%= key %>" ><%= value %></div>
  <% end %>
<% end %>
<%= yield %>

</body>
</html>

